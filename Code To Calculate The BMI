 //PURPOSE: CALCULATE THE BMI


#include<iostream>

using namespace std;

int main ()

{
    float bmi, height, weight;
    cout<<"Enter your weight (kg) ";
    cin>>weight;

    cout<<"Enter your height (meter)";
    cin>> height;

    bmi=weight/(height*height);

    cout<<"Body Mass Index = "<<bmi;

return 0;


}
